<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" 
  assembly="Samsara.TIConsulting.Core" 
  namespace="Samsara.TIConsulting.Core.Entities">
  
  <class name="ServerConsultingOldServerComputer" table="TIConsulting.ServerConsultingOldServerComputers">
    
    <id name="ServerConsultingOldServerComputerId" type="int">
      <generator class="native"/>
    </id>

    <property name="ServerModel"/>
    <property name="ServersQuantity"/>
    <property name="ServerSpecs"/>

    <property name="UpdatedBy"/>
    <property name="CreatedBy"/>
    <property name="CreatedOn" type="Timestamp"/>
    <property name="UpdatedOn" type="Timestamp"/>
    <property name="Activated"/>
    <property name="Deleted"/>

    <many-to-one name="ServerConsulting"
                 class="Samsara.TIConsulting.Core.Entities.ServerConsulting, Samsara.TIConsulting.Core"
                 column="ServerConsultingId" />

    <many-to-one name="ServerComputerType" lazy="false"
                 class="Samsara.CustomerContext.Core.Entities.ServerComputerType, Samsara.CustomerContext.Core"
                 column="ServerComputerTypeId" />

    <many-to-one name="ComputerBrand" lazy="false"
                 class="Samsara.CustomerContext.Core.Entities.ComputerBrand, Samsara.CustomerContext.Core"
                 column="ComputerBrandId" />

    <many-to-one name="OperativeSystem" lazy="false"
                 class="Samsara.CustomerContext.Core.Entities.OperativeSystem, Samsara.CustomerContext.Core"
                 column="OperativeSystemId" />

    <many-to-one name="RackType" lazy="false"
                 class="Samsara.CustomerContext.Core.Entities.RackType, Samsara.CustomerContext.Core"
                 column="RackTypeId" />
    
  </class>

  <query name="ServerConsultingOldServerComputer.GetByParameters">
    <![CDATA[
        from ServerConsultingOldServerComputer where Deleted = 0 and Activated = 1
    ]]>
  </query>

  <query name="ServerConsultingOldServerComputer.GetListByParameters">
    <![CDATA[
        from ServerConsultingOldServerComputer 
        where Deleted = 0 and Activated = 1
    ]]>
  </query>

  <query name="ServerConsultingOldServerComputer.SearchByParameters">
    <![CDATA[
        from ServerConsultingOldServerComputer 
        where Deleted = 0 and Activated = 1
        and ServerConsultingOldServerComputerId = :ServerConsultingOldServerComputerId
    ]]>
  </query>

</hibernate-mapping>