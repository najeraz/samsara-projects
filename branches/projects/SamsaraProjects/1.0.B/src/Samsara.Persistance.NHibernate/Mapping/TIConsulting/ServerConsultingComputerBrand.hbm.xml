<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" 
  assembly="Samsara.TIConsulting.Core" 
  namespace="Samsara.TIConsulting.Core.Entities">
  
  <class name="ServerConsultingComputerBrand" table="TIConsulting.ServerConsultingComputerBrands">
    
    <id name="ServerConsultingComputerBrandId" type="int">
      <generator class="native"/>
    </id>

    <property name="UpdatedBy"/>
    <property name="CreatedBy"/>
    <property name="CreatedOn" type="Timestamp"/>
    <property name="UpdatedOn" type="Timestamp"/>
    <property name="Activated"/>
    <property name="Deleted"/>

    <many-to-one name="ServerConsulting"
                 class="Samsara.TIConsulting.Core.Entities.ServerConsulting, Samsara.TIConsulting.Core"
                 column="ServerConsultingId" />
    
    <many-to-one name="ComputerBrand" lazy="false"
                 class="Samsara.CustomerContext.Core.Entities.ComputerBrand, Samsara.CustomerContext.Core"
                 column="ComputerBrandId" />
    
  </class>

  <query name="ServerConsultingComputerBrand.GetByParameters">
    <![CDATA[
        from ServerConsultingComputerBrand where Deleted = 0 and Activated = 1
    ]]>
  </query>

  <query name="ServerConsultingComputerBrand.GetListByParameters">
    <![CDATA[
        from ServerConsultingComputerBrand 
        where Deleted = 0 and Activated = 1
    ]]>
  </query>

  <query name="ServerConsultingComputerBrand.SearchByParameters">
    <![CDATA[
        from ServerConsultingComputerBrand 
        where Deleted = 0 and Activated = 1
        and ServerConsultingComputerBrandId = :ServerConsultingComputerBrandId
    ]]>
  </query>

</hibernate-mapping>