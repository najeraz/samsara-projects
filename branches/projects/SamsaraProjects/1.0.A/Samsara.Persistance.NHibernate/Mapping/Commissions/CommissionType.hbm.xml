<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" 
  assembly="Samsara.Commissions.Core" 
  namespace="Samsara.Commissions.Core.Entities">
  
  <class name="CommissionType" table="Commissions.CommissionTypes">
    
    <id name="CommissionTypeId" type="int">
      <generator class="native"/>
    </id>

    <property name="Description"/>
    <property name="Name"/>

    <property name="UpdatedBy"/>
    <property name="CreatedBy"/>
    <property name="CreatedOn"/>
    <property name="UpdatedOn"/>
    <property name="Activated"/>
    <property name="Deleted"/>
    
  </class>

  <query name="CommissionType.GetByParameters">
    <![CDATA[
        from CommissionType where Deleted = 0 and Activated = 1
    ]]>
  </query>

  <query name="CommissionType.GetListByParameters">
    <![CDATA[
        from CommissionType where Deleted = 0 and Activated = 1
    ]]>
  </query>

  <query name="CommissionType.SearchByParameters">
    <![CDATA[
        from CommissionType where Deleted = 0 and Activated = 1
    ]]>
  </query>


</hibernate-mapping>