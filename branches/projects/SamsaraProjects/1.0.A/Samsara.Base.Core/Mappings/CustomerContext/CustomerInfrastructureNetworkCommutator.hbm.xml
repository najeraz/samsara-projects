<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" 
  assembly="Samsara.CustomerContext.Core" 
  namespace="Samsara.CustomerContext.Core.Entities">
  
  <class name="CustomerInfrastructureNetworkCommutator" table="CustomerContext.CustomerInfrastructureNetworkCommutators">
    
    <id name="CustomerInfrastructureNetworkCommutatorId" type="int">
      <generator class="native"/>
    </id>

    <property name="NumberOfTrunks"/>
    <property name="NumberOfExtensions"/>
    <property name="Deleted"/>
    <property name="Activated"/>

    <many-to-one name="CustomerInfrastructureNetwork"
                 class="Samsara.CustomerContext.Core.Entities.CustomerInfrastructureNetwork, Samsara.CustomerContext.Core"
                 column="CustomerInfrastructureNetworkId"/>

    <many-to-one name="CommutatorType"
                 class="Samsara.CustomerContext.Core.Entities.CommutatorType, Samsara.CustomerContext.Core"
                 column="CommutatorTypeId"/>

    <many-to-one name="CommutatorBrand"
                 class="Samsara.CustomerContext.Core.Entities.CommutatorBrand, Samsara.CustomerContext.Core"
                 column="CommutatorBrandId"/>

  </class>

  <query name="CustomerInfrastructureNetworkCommutator.GetByParameters">
    <![CDATA[
        from CustomerInfrastructureNetworkCommutator
        where Deleted = 0 and Activated = 1
    ]]>
  </query>

  <query name="CustomerInfrastructureNetworkCommutator.GetListByParameters">
    <![CDATA[
        from CustomerInfrastructureNetworkCommutator
        where Deleted = 0 and Activated = 1
        and CustomerInfrastructureNetworkId = :CustomerInfrastructureNetworkId
    ]]>
  </query>

  <query name="CustomerInfrastructureNetworkCommutator.SearchByParameters">
    <![CDATA[
        from CustomerInfrastructureNetworkCommutator
        where Deleted = 0 and Activated = 1
        and CustomerInfrastructureNetworkId = :CustomerInfrastructureNetworkId
    ]]>
  </query>

</hibernate-mapping>