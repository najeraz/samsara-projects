<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" 
  assembly="Samsara.CustomerContext.Core" 
  namespace="Samsara.CustomerContext.Core.Entities">
  
  <class name="CommutatorType" table="CustomerContext.CommutatorTypes">
    
    <id name="CommutatorTypeId" type="int">
      <generator class="native"/>
    </id>

    <property name="Name"/>
    <property name="Description"/>
    <property name="Deleted"/>
    <property name="Activated"/>

  </class>

  <query name="CommutatorType.GetByParameters">
    <![CDATA[
        from CommutatorType where Deleted = 0 and Activated = 1
    ]]>
  </query>

  <query name="CommutatorType.GetListByParameters">
    <![CDATA[
        from CommutatorType where Deleted = 0 and Activated = 1
    ]]>
  </query>

  <query name="CommutatorType.SearchByParameters">
    <![CDATA[
        from CommutatorType where Deleted = 0 and Activated = 1
    ]]>
  </query>

</hibernate-mapping>