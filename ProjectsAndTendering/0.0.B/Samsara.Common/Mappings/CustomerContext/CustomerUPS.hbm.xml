<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" 
  assembly="Samsara.CustomerContext.Core" 
  namespace="Samsara.CustomerContext.Core.Entities">
  
  <class name="CustomerUPS" table="CustomerContext.CustomerUPSs">

    <id name="CustomerUPSId" type="int">
      <generator class="native"/>
    </id>

    <property name="Capacity"/>
    
    <many-to-one name="UPSType"
                 class="Samsara.CustomerContext.Core.Entities.UPSType, Samsara.CustomerContext.Core"
                 column="UPSTypeId"/>

    <many-to-one name="UPSBrand"
                 class="Samsara.CustomerContext.Core.Entities.CustomerUPSType, Samsara.CustomerContext.Core"
                 column="CustomerUPSTypeId"/>
    
  </class>

  <query name="CustomerUPS.GetByParameters">
    <![CDATA[
        from CustomerUPS where Deleted = 0 and Activated = 1
    ]]>
  </query>

  <query name="CustomerUPS.GetListByParameters">
    <![CDATA[
        from CustomerUPS where Deleted = 0 and Activated = 1
    ]]>
  </query>

  <query name="CustomerUPS.SearchByParameters">
    <![CDATA[
        from CustomerUPS where Deleted = 0 and Activated = 1
    ]]>
  </query>

</hibernate-mapping>