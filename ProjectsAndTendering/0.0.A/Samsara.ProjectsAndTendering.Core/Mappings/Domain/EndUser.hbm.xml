<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" 
  assembly="Samsara.ProjectsAndTendering.Core" 
  namespace="Samsara.ProjectsAndTendering.Core.Entities.Domain">
  
  <class name="EndUser" table="EndUsers">
    
    <id name="EndUserId" type="int">
      <generator class="native"/>
    </id>
    
    <property name="DependencyId"/>
    <property name="Name"/>
    <property name="Deleted"/>
    <property name="Activated"/>

  </class>

  <query name="LoadEndUsers">
    <![CDATA[
        from EndUser where Deleted = 0 and Activated = 1
    ]]>
  </query>

  <query name="LoadEndUsersByDependencyId">
    <![CDATA[
        from EndUser where DependencyId = :DependencyId and Deleted = 0 and Activated = 1
    ]]>
  </query>

</hibernate-mapping>