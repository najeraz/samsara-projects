<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" 
  assembly="Samsara.ProjectsAndTendering.Core" 
  namespace="Samsara.ProjectsAndTendering.Core.Entities.Domain">
  
  <class name="TenderManufacturers" table="TenderManufacturers">
    
    <composite-id>
      <key-property name="TenderId" column="TenderId" type="int"></key-property>
      <key-property name="ManufacturerId" column="ManufacturerId" type="int"></key-property>
    </composite-id>
    
    <property name="FolioReference"/>
    <many-to-one name="Manufacturer"
                 class="Samsara.ProjectsAndTendering.Core.Entities.Domain.Manufacturer, Samsara.ProjectsAndTendering.Core"
                 column="ManufacturerId"/>
    
  </class>

</hibernate-mapping>