<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" 
  assembly="Samsara.ProjectsAndTendering.Core" 
  namespace="Samsara.ProjectsAndTendering.Core.Entities.Domain">
  
  <class name="TenderLinesManufacturers" table="TenderLinesManufacturers">
    
    <composite-id>
      <key-property name="TenderLineId" column="TenderLineId" type="int"></key-property>
      <key-property name="ManufacturerId" column="ManufacturerId" type="int"></key-property>
    </composite-id>

    <property name="Price"/>
    <property name="Description"/>
    <property name="Model"/>
    <property name="Deleted"/>
    <property name="Activated"/>
    
  </class>

  <query name="TenderLinesManufacturers.SearchByParameters">
    <![CDATA[
        from TenderLinesManufacturers where Deleted = 0 and Activated = 1
        and TenderLineId = :TenderLineId
    ]]>
  </query>

</hibernate-mapping>