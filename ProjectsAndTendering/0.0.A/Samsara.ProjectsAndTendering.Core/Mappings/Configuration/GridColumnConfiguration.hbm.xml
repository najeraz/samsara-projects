<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" 
  assembly="Samsara.ProjectsAndTendering.Core" 
  namespace="Samsara.ProjectsAndTendering.Core.Entities.Configuration">
  
  <class name="GridColumnConfiguration" table="GridColumnConfigurations">

    <id name="GridColumnConfigurationId" type="int">
      <generator class="native"/>
    </id>

    <many-to-one name="GridConfiguration"
                 class="Samsara.ProjectsAndTendering.Core.Entities.Configuration.GridConfiguration, Samsara.ProjectsAndTendering.Core"
                 column="GridConfigurationId"/>
    
    <property name="ColumnName"/>
    <property name="ColumnEndUserName"/>
    <property name="Band"/>
    <property name="Visible"/>

  </class>

</hibernate-mapping>