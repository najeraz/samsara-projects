<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" 
  assembly="Samsara.Commissions.Core" 
  namespace="Samsara.Commissions.Core.Entities">
  
  <class name="CommissionPaymentStaff" table="Commissions.CommissionPaymentStaffs">
    
    <id name="CommissionPaymentStaffId" type="int">
      <generator class="native"/>
    </id>

    <property name="UpdatedBy"/>
    <property name="CreatedBy"/>
    <property name="CreatedOn" type="Timestamp"/>
    <property name="UpdatedOn" type="Timestamp"/>
    <property name="Activated"/>
    <property name="Deleted"/>

    <many-to-one name="CommissionPayment"
                 class="Samsara.Commissions.Core.Entities.CommissionPayment, Samsara.Commissions.Core"
                 column="CommissionPaymentId"/>

    <many-to-one name="Staff" lazy="false"
                 class="Samsara.AlleatoERP.Core.Entities.Staff, Samsara.AlleatoERP.Core"
                 column="StaffId"/>
    
  </class>

  <query name="CommissionPaymentStaff.GetByParameters">
    <![CDATA[
        from CommissionPaymentStaff where Deleted = 0 and Activated = 1
    ]]>
  </query>

  <query name="CommissionPaymentStaff.GetListByParameters">
    <![CDATA[
        from CommissionPaymentStaff where Deleted = 0 and Activated = 1
    ]]>
  </query>

  <query name="CommissionPaymentStaff.SearchByParameters">
    <![CDATA[
        from CommissionPaymentStaff where Deleted = 0 and Activated = 1
    ]]>
  </query>


</hibernate-mapping>