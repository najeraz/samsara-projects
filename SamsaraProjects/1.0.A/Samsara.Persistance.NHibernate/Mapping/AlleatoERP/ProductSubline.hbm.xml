<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
  assembly="Samsara.AlleatoERP.Core"
  namespace="Samsara.AlleatoERP.Core.Entities">
  
  <class name="ProductSubline" table="Sublineas_Articulos">

    <id name="ProductSublineId" column="sublinea" type="int">
    </id>
    
    <property name="Name" column="nombre_sublinea"/>
    
  </class>

  <query name="ProductSubline.GetByParameters">
    <![CDATA[
        from ProductSubline
    ]]>
  </query>

  <query name="ProductSubline.GetListByParameters">
    <![CDATA[
        from ProductSubline
    ]]>
  </query>

  <query name="ProductSubline.SearchByParameters">
    <![CDATA[
        from ProductSubline 
        where Name like :Name
    ]]>
  </query>

</hibernate-mapping>