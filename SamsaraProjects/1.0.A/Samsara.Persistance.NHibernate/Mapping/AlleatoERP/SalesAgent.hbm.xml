<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
  assembly="Samsara.AlleatoERP.Core"
  namespace="Samsara.AlleatoERP.Core.Entities">
  
  <class name="SalesAgent" table="lineas_articulos">

    <id name="SalesAgentId" type="int">
    </id>
    
    <property name="Name"/>
    
  </class>

  <query name="SalesAgent.GetByParameters">
    <![CDATA[
        from SalesAgent
    ]]>
  </query>

  <query name="SalesAgent.GetListByParameters">
    <![CDATA[
        from SalesAgent
    ]]>
  </query>

  <query name="SalesAgent.SearchByParameters">
    <![CDATA[
        from SalesAgent 
        where Name like :Name
    ]]>
  </query>

</hibernate-mapping>