<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" 
  assembly="Samsara.ProjectsAndTendering.Core" 
  namespace="Samsara.ProjectsAndTendering.Core.Entities">
  
  <class name="BidderType" table="ProjectsAndTendering.BidderTypes">
    
    <id name="BidderTypeId" type="int">
      <generator class="native"/>
    </id>

    <property name="Name"/>
    <property name="Deleted"/>
    <property name="Activated"/>

  </class>

  <query name="BidderType.GetListByParameters">
    <![CDATA[
        from BidderType where Deleted = 0 and Activated = 1
    ]]>
  </query>


</hibernate-mapping>