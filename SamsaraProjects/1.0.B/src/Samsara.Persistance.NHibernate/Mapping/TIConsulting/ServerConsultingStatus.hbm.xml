<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" 
  assembly="Samsara.TIConsulting.Core" 
  namespace="Samsara.TIConsulting.Core.Entities">
  
  <class name="ServerConsultingStatus" table="TIConsulting.ServerConsultingStatus">
    
    <id name="ServerConsultingStatusId" type="int">
      <generator class="native"/>
    </id>

    <property name="Name"/>

    <property name="UpdatedBy"/>
    <property name="CreatedBy"/>
    <property name="CreatedOn" type="Timestamp"/>
    <property name="UpdatedOn" type="Timestamp"/>
    <property name="Activated"/>
    <property name="Deleted"/>
    
  </class>

  <query name="ServerConsultingStatus.GetByParameters">
    <![CDATA[
        from ServerConsultingStatus where Deleted = 0 and Activated = 1
    ]]>
  </query>

  <query name="ServerConsultingStatus.GetListByParameters">
    <![CDATA[
        from ServerConsultingStatus where Deleted = 0 and Activated = 1
    ]]>
  </query>

  <query name="ServerConsultingStatus.SearchByParameters">
    <![CDATA[
        from ServerConsultingStatus where Deleted = 0 and Activated = 1
    ]]>
  </query>

</hibernate-mapping>